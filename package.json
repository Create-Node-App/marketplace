{
  "name": "marketplace",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build:dev": "webpack --env env=development",
    "build:dev:watch": "yarn build:dev --watch --hot",
    "build:dev:analyze": "yarn build:dev --env addon=bundleanalyze --env addon=bundlevisualizer",
    "build": "webpack --env env=production",
    "build:watch": "yarn build --watch",
    "build:analyze": "yarn build --env addon=bundleanalyze --env addon=bundlevisualizer",
    "lint": "prettier --ignore-path .eslintignore --check \"**/*.{js,jsx,ts,tsx,json,css,sass,scss,less,html,md}\" && eslint src",
    "lint:fix": "prettier --ignore-path .eslintignore --write \"**/*.{js,jsx,ts,tsx,json,css,sass,scss,less,html,md}\" && eslint src --fix",
    "serve:dev": "webpack-dev-server --mode development --open --env env=development",
    "serve:dev:dashboard": "webpack-dashboard webpack-dev-server -- --mode development --env addon=dashboard",
    "start": "yarn serve:dev",
    "serve": "yarn build && serve -s -C build",
    "test": "jest --runInBand --detectOpenHandles --passWithNoTests",
    "test:watch": "jest -u --runInBand --verbose --watch --detectOpenHandles --passWithNoTests",
    "test:coverage": "jest -u --coverage --verbose --runInBand --detectOpenHandles --passWithNoTests",
    "docker:dev": "yarn docker:dev:build && yarn docker:dev:start",
    "docker:dev:build": "docker build -f docker/web/Dockerfile --target development -t marketplace .",
    "docker:dev:start": "docker run --rm -it --network host -v $PWD:/usr/src/app marketplace",
    "docker:prod": "yarn docker:prod:build && yarn docker:prod:start",
    "docker:prod:build": "docker build -f docker/web/Dockerfile --target production -t marketplace:production .",
    "docker:prod:start": "docker run --rm -it --network host -v $PWD:/usr/src/app marketplace:production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "yarn lint:fix"
    ],
    "*.{json,css,sass,scss,less,html,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "yarn lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "highlight.js": "^11.5.1",
    "i18next": "^21.8.8",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "marked": "^4.0.16",
    "moment-timezone": "^0.5.34",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.17.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^28.1.1",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.39",
    "@types/react": "^18.0.11",
    "@types/react-dom": "^18.0.5",
    "@types/react-icons": "^3.0.0",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "esbuild-loader": "^2.19.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "inject-html-webpack-plugin": "^1.0.1",
    "jest": "^28.1.0",
    "jest-transform-stub": "^2.0.0",
    "less": "^4.1.2",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "path": "^0.12.7",
    "prettier": "^2.6.2",
    "sass-loader": "^13.0.0",
    "serve": "^13.0.2",
    "sinon": "^14.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3",
    "url-loader": "1.1.2",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dashboard": "^3.3.7",
    "webpack-dev-server": "^4.9.1",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.8.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
